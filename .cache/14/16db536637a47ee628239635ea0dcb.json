\n}\n\n.container {\n    position: relative;\n    /* width: 70%; */\n    padding: 0px;\n    margin: 0px;\n  }\n  \n  .image {\n    opacity: 1;\n    display: block;\n    width: 100%;\n    height: auto;\n    transition: .5s ease;\n    backface-visibility: hidden;\n    margin:0px;\n  }\n  \n  .middle {\n    transition: .5s ease;\n    opacity: 0;\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    -ms-transform: translate(-50%, -50%);\n    text-align: center;\n  }\n  \n  .container:hover .image {\n    opacity: 0.3;\n  }\n  \n  .container:hover .middle {\n    opacity: 1;\n  }\n  \n  .text {\n    background-color: #034929;\n    color: white;\n    font-size: 16px;\n    padding: 16px 32px;\n    border-radius: 5%;\n    box-shadow: 0 8px 6px -6px black;\n  }\n\n  .text p{\n      color: white;\n  }\n\n  .Project-items{\n      display: flex;\n      flex-basis: 50%;\n      flex-direction: column;\n      text-align: left;\n      padding: 0px;\n      Margin: 50px 5px;\n  }\n\n/* #endregion Projects */\n\n/* #region Contact */\n\n#main {\n    border: 5px solid #034929;\n}\n\n.form-group {\n    padding: 20px;\n    margin: 10px;\n    font-size: 20px;\n}\n\n.form-group input[type=submit] {\n    background-color: #034929;\n    color: white;\n}\n\n.form-group input[type=submit]:hover {\n    background-color: seagreen;\n}\n/* #endregion Contact */\n\n/* #region Footer */\nfooter{\n    color:#082015;\n    background: #f5faf8;\n}\n\nimg{\n    width:25px;\n    height:25px;\n    text-align: match-parent;\n}\n\n.footer-list-item {\n    display: inline;\n}\n\n#glyphs-row{\n    margin-left: 31%;\n}\n/* #endregion Footer */\n\n/* //Image styles */\n\n"},"lineCount":null}},"error":null,"hash":"9602bd92a76c90a36fd255355cb02f2e","cacheData":{"env":{}}}